{
  "manifest_version": 2,
  "name": "User-Agent",
  "version": "1.0",
  "icons": {
    "48": "link-48.png"
  },
  "applications": {
    "gecko": {
    "id": "user-agent@floorp.ablaze.one"
		}
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  "experiment_apis": {
    "BrowserInfo": {
      "schema": "APIs/BrowserInfo.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "APIs/BrowserInfo.js",
        "paths": [["BrowserInfo"]]
      }
    }
  }
}

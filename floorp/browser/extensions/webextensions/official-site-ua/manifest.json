{
    "description": "Floorpで使用されているウェブパネルのUAをモバイル向けに上書きします。",
    "manifest_version": 2,
    "name": "official-site-ua",
    "version": "1.0",
    "icons": {
        "48": "link-48.png"
      },
    "applications": {
        "gecko": {
            "id": "official-site-ua@floorp.ablaze.one",
            "strict_min_version": "102.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "inject.js"
    ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
